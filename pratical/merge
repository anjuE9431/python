# Input dictionaries from the user
dict1 = {}
dict2 = {}

# Get the number of key-value pairs for the first dictionary
n1 = int(input("Enter the number of items in the first dictionary: "))
print("Enter items for the first dictionary:")
for _ in range(n1):
    key = input("Enter key: ")
    value = input("Enter value: ")
    dict1[key] = value

# Get the number of key-value pairs for the second dictionary
n2 = int(input("Enter the number of items in the second dictionary: "))
print("Enter items for the second dictionary:")
for _ in range(n2):
    key = input("Enter key: ")
    value = input("Enter value: ")
    dict2[key] = value

# Merge dictionaries
merged_dict = dict1.copy()
merged_dict.update(dict2)

# Output the merged dictionary
print("Merged dictionary:", merged_dict)
